{
    "manifestVersion": 1,
    "id": "devops-result-html-in-release-environment",
    "name": "DevOps Result",
    "version": "0.0.44",
    "publisher": "dpolania",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Tools for building/releasing with DevOps. Includes build/release tasks.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "scopes": [
        "vso.code_full"
    ],
    "files": [
        {
            "path": "publishReport"
        }
    ],
    "contributions": [
        {
            "id": "publish-report-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "publishReport"
            }
        }
    ]
}